{
  "name": "@cst-test/cold-start-tool",
  "version": "1.0.3",
  "description": "Check your lambdas file size and optimize them",
  "main": "index.ts",
  "scripts": {
    "start": "npx ts-node index.ts",
    "build": "tsc",
    "postinstall": "tsc && cp ./cst-config.json ../../../cst-config.json && node ./dist/src/install/add-script.js",
    "preuninstall": "rm ../../../cst-config.json && node ./dist/src/install/remove-script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theapexlab/cold-start-tool.git"
  },
  "keywords": [
    "serverless",
    "cold start"
  ],
  "author": "Apex Lab",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/theapexlab/cold-start-tool/issues"
  },
  "homepage": "https://github.com/theapexlab/cold-start-tool#readme",
  "dependencies": {
    "@types/node": "^20.2.1",
    "typescript": "^5.0.4"
  }
}
